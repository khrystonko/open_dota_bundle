{
    "type": "symfony-bundle",
    "name": "khrystonko/opendota",
    "description": "OpenDota bundle",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.1",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/lts": "^4@dev",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "khrystonko\\OpenDotaBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "khrystonko\\OpenDotaBundle\\Tests\\": "tests"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
